{
    "name": "kingdot",
    "version": "1.0.57",
    "description": "A UI Components Library For Vue",
    "author": "kingsoft",
    "main": "lib/kingdot.js",
    "unpkg": "lib/index.js",
    "style": "lib/theme-default/index.css",
    "license": "MIT",
    "files": [
        "lib",
        "components",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:ksyun-fe/kingdot.git"
    },
    "homepage": "https://ksyun-fe.github.io/kingdot",
    "keywords": [
        "components",
        "vue",
        "kingsoft"
    ],
    "bugs": {
        "url": "https://github.com/ksyun-fe/kingdot/issues"
    },
    "scripts": {
        "build:entry": "node build/bin/build-index.js & node build/bin/iconInit.js",
        "dev": "npm run build:entry && cross-env NODE_ENV=development THEME=theme-default webpack-dev-server  --config build/webpack.demo.conf.js",
        "dev:theme": "npm run build:entry && cp-cli src/styles/theme-default/src src/styles/theme/src && cross-env NODE_ENV=development THEME=theme webpack-dev-server  --config build/webpack.demo.conf.js",
        "clean": "rimraf lib && rimraf src/*/lib && rimraf test/**/coverage",
        "lint": "eslint --ext .js,.vue src components build examples",
        "dist:locale": "babel src --out-dir lib --ignore src/index.js",
        "dist:theme": "npm run build:entry && cp-cli src/styles/theme-default/src src/styles/theme/src && node build/bin/gen-cssfile.js && gulp build --gulpfile src/styles/gulpfile.js && cp-cli src/styles/lib lib/",
        "demo:build": "rimraf docs && npm run build:entry && cross-env NODE_ENV=production webpack --config build/webpack.demo.conf.js && gulp build --gulpfile build/bin/gen-docstheme.js",
        "dist:component": "cross-env NODE_ENV=production webpack --config build/webpack.component.js",
        "dist:prod": "npm run build:entry && cross-env NODE_ENV=production webpack --config build/webpack.config.js",
        "dist:commonjs": "npm run build:entry && cross-env NODE_ENV=production webpack --config build/webpack.common.js",
        "dist": "npm run clean && npm run build:entry && npm run lint && npm run dist:theme && npm run dist:locale && npm run dist:prod && npm run dist:commonjs && npm run dist:component",
        "release": "sh build/release.sh",
        "test": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "test:watch": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
        "lang": "node build/bin/lang.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitLint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{js,vue}": [
            "eslint"
        ]
    },
    "dependencies": {
        "@popperjs/core": "^2.5.3",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "dayjs": "^1.9.1",
        "lodash": "^4.17.4",
        "normalize-wheel": "^1.0.1",
        "promise-polyfill": "8.2.0",
        "tinycolor2": "^1.4.2",
        "ulid": "^2.3.0"
